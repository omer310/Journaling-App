{
  "expo": {
    "name": "Soul Pages",
    "slug": "mobile",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "soulpages",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "statusBar": {
      "style": "light",
      "backgroundColor": "#1a1a1a",
      "translucent": false
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.omer310.soulpages",
      "statusBar": {
        "barStyle": "light-content",
        "backgroundColor": "#1a1a1a"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.omer310.soulpages",
      "statusBar": {
        "barStyle": "light-content",
        "backgroundColor": "#1a1a1a",
        "translucent": false
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "acf5ade7-620b-46e2-9e95-363ff4695794"
      }
    }
  }
}
